{
    "name" : "rev42/revpdf-gui",
    "type" : "library",
    "description" : "Graphical PDF generation tool for PHP",
    "keywords" : ["revpdf", "pdf", "php"],
    "homepage" : "http://www.revpdf.org",
    "version": "0.9.8-dev",
    "license" : "GPL-3.0",
    "authors" : [
        {
            "name": "Olivier Cornu",
            "email": "contact@revpdf.org"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name" : "jquery/qunit",
                "version" : "1.8.0",
                "source" : {
                    "url" : "https://github.com/jquery/qunit.git",
                    "type" : "git",
                    "reference" : "v1.8.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name" : "gyoshev/qhint",
                "version" : "master",
                "source" : {
                    "url" : "https://github.com/gyoshev/qhint.git",
                    "type" : "git",
                    "reference" : "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name" : "jshint/jshint",
                "version" : "master",
                "source" : {
                    "url" : "https://github.com/jshint/jshint.git",
                    "type" : "git",
                    "reference" : "master"
                }
            }
        }
    ],
    "require": {
        "php"                   : ">=5.3.3",
        "silex/silex"           : "~1.0.0",
        "rev42/revpdf-lib"      : "dev-master",
        "twig/twig"             : ">=1.8.0,<2.0-dev",
        "monolog/monolog"       : "~1.9.0",
        "symfony/form"          : "~2.3.0",
        "symfony/translation"   : "~2.3.0",
        "symfony/twig-bridge"   : "~2.3.0",
        "symfony/validator"     : "~2.3.0",
        "symfony/yaml"          : "~2.3.0",
        "symfony/config"        : "~2.3.0",
        "kriswallsmith/assetic" : "~1.0.0",
        "symfony/security"      : "~2.3.0",
        "symfony/finder"        : "~2.3.0",
        "mheap/silex-assetic"   : "~1.0.0",
        "doctrine/dbal"         : "~2.2.0",
        "lightopenid/lightopenid" : "~0.7.0",
        "knplabs/repository-service-provider" : "dev-master",
        "twitter/bootstrap"     : "2.2.2",
        "swiftmailer/swiftmailer": "~5.1.0",
        "symfony/console"       : "~2.3.0",
        "symfony/process"       : "~2.3.0"
    },
    "require-dev": {
        "twig/extensions": "dev-master",
        "symfony/dom-crawler" : "~2.3",
        "symfony/css-selector" : "~2.3",
        "symfony/browser-kit" : "~2.3",
        "jquery/qunit" : "1.8.0",
        "gyoshev/qhint" : "dev-master",
        "jshint/jshint" : "master",
        "symfony/Filesystem" : "~2.3"
    },
    "config": {
        "process-timeout": 500
    },
    "autoload": {
        "psr-0": {
            "RevPDF": "src/"
        }
    }
}
