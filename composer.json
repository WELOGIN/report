{
    "name" : "rev42/revpdf-gui",
    "type" : "library",
    "description" : "Graphical PDF generation tool for PHP",
    "keywords" : ["revpdf", "pdf", "php"],
    "homepage" : "http://www.revpdf.org",
    "version": "0.9.8-dev",
    "license" : "GPL-3.0",
    "authors" : [
        {
            "name": "Olivier Cornu",
            "email": "contact@revpdf.org"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name" : "jquery/qunit",
                "version" : "1.8.0",
                "source" : {
                    "url" : "https://github.com/jquery/qunit.git",
                    "type" : "git",
                    "reference" : "v1.8.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name" : "gyoshev/qhint",
                "version" : "master",
                "source" : {
                    "url" : "https://github.com/gyoshev/qhint.git",
                    "type" : "git",
                    "reference" : "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name" : "jshint/jshint",
                "version" : "master",
                "source" : {
                    "url" : "https://github.com/jshint/jshint.git",
                    "type" : "git",
                    "reference" : "master"
                }
            }
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php"                   : ">=5.3.3",
        "silex/silex"           : ">=1.1.0,<1.2",
        "rev42/revpdf-lib"      : "~2.1",
        "twig/twig"             : ">=1.8.0,<2.0-dev",
        "monolog/monolog"       : "~1.4,>=1.4.1",
        "symfony/form"          : ">=2.3,<2.5-dev",
        "symfony/translation"   : ">=2.3,<2.5-dev",
        "symfony/twig-bridge"   : ">=2.3,<2.5-dev",
        "symfony/validator"     : ">=2.3,<2.5-dev",
        "symfony/yaml"          : ">=2.3,<2.5-dev",
        "symfony/config"        : ">=2.3,<2.5-dev",
        "kriswallsmith/assetic" : "1.0.*",
        "symfony/security"      : ">=2.3,<2.5-dev",
        "symfony/finder"        : ">=2.3,<2.5-dev",
        "mheap/silex-assetic"   : "dev-master@dev",
        "doctrine/dbal"         : ">=2.2.0,<2.4.0-dev",
        "fp/lightopenid"        : "dev-master",
        "knplabs/repository-service-provider" : "dev-master",
        "twitter/bootstrap"     : "2.2.2",
        "swiftmailer/swiftmailer": "5.*",
        "symfony/console"       : ">=2.3,<2.5-dev",
        "symfony/process"       : ">=2.3,<2.5-dev"
    },
    "require-dev": {
        "twig/extensions": "dev-master",
        "symfony/dom-crawler" : "~2.2",
        "symfony/css-selector" : "~2.2",
        "symfony/browser-kit" : "~2.2",
        "jquery/qunit" : "1.8.0",
        "gyoshev/qhint" : "dev-master",
        "jshint/jshint" : "master",
        "symfony/Filesystem" : "~2.2"
    },
    "config": {
        "process-timeout": "500"
    },
    "autoload": {
        "psr-0": {
            "RevPDF": "src/"
        }
    }
}
